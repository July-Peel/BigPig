<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Static\1.jpg" />
    <None Remove="Static\2.jpg" />
    <None Remove="Static\bainian.json" />
    <None Remove="Static\CC.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FullControls" Version="1.5.0" />
    <PackageReference Include="HtmlAgilityPack" Version="1.11.34" />
    <PackageReference Include="MaterialDesignThemes" Version="4.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Windows.Interactivity">
      <HintPath>bin\System.Windows.Interactivity.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Static\1.jpg" />
    <Resource Include="Static\2.jpg" />
    <EmbeddedResource Include="Static\bainian.json">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
    <Resource Include="Static\CC.png" />
  </ItemGroup>

</Project>
